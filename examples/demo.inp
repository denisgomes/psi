#! /usr/bin

# parameters
L = 10 * 12

# create model
mdl = Model('demo')

# define properties
pipe1 = Pipe.from_file('pipe1', '10', '40')
mat1 = Material.from_file('mat1', 'A53A', 'B31.1')

# create geometry
pt10 = Point(10)
run20 = Run(20, L)

# assign supports
anc1 = Anchor('A1', 10)
anc1.apply([run20])

# define loads for operating case 1
w1 = Weight('W1', 1)
w1.apply([run20])

p1 = Pressure('P1', 1, 250)
p1.apply([run20])

# define a loadcase
l1 = LoadCase('l1', 'sus', [Weight, Pressure], [1, 1])
l2 = LoadCase('l2', 'sus', [Weight, Pressure], [1, 1])
l3 = LoadComb('l3', 'sus', 'srss', [l1, l2], [1, 1])

# code
b311 = B311('B31.1')
b311.apply([run20])

# run the analysis
mdl.analyze()
# mdl.settings.units = 'si'

# postprocess
# disp = Movements('r1', [l1, l2])
# disp.to_screen()

forc = Forces('r2', [l1])
forc.to_screen()

reac = Reactions('r3', [l1])
reac.to_screen()

str1 = Stresses('r4', [l1, l2])
str1.to_screen()

# disp2 = Movements('r5', [l3])
# disp2.to_screen()

cod1 = Codes('r6', [l1, l2])
cod1.to_screen()
