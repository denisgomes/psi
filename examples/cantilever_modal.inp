"""Simple modal analysis of a cantilevered pipe"""

# parameter
L = 1*12

mdl = Model('simple')

# properties
pipe1 = Pipe.from_file('pipe1', '3', '40')
mat1 = Material.from_file('mat1', 'A53A', 'B31.1')

# geometry
pt10 = Point(10)
run20 = Run(20, L)

# supports
anc1 = Anchor('A1', 10)
anc1.apply([run20])

# code flexibility
b311 = B311('B311')
b311.apply([run20])

# modal analysis
mdl.analyze(mode=2)
